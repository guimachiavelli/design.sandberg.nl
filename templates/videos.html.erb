<% if @videos %>
    <ul class="image-listing">
        <% @videos.each do |video| %>
            <li class="image-listing__item">
                <iframe class="image-listing__frame" src="https://player.vimeo.com/video/<%= video %>?color=000000&title=0&byline=0&portrait=0" frameborder="0" allowfullscreen></iframe>
            </li>
        <% end %>
    </ul>
<% end %>

